# App
APP_NAME=MyNestApp
APP_ENV=development
APP_PORT=3000
APP_DEBUG=true

# Node
NODE_ENV=development

# Database (Postgres example)
DB_TYPE=postgres
DB_HOST=localhost
DB_PORT=5432
DB_USERNAME=postgres
DB_PASSWORD=your_db_password
DB_NAME=my_nest_db
DB_SYNCHRONIZE=false     # true only in dev
DB_LOGGING=false

# JWT / Auth
JWT_SECRET=replace_with_a_strong_secret
JWT_EXPIRES_IN=3600s

